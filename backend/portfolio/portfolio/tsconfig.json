{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS", 
    "strict": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "esModuleInterop": true, 
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist"
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}
